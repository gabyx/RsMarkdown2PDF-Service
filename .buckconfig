[cells]
root = .
prelude = buck/prelude-nix
prelude-nix = buck/prelude-nix
; prelude-meta = buck/prelude-meta

bxl = buck/prelude-nix/bxl

[buildfile]
name = BUILD

[build]
execution_platforms = prelude-nix//platform:default

[parser]
target_platform_detector_spec = target:root//...->prelude-nix//platform:default target:prelude-nix//...->prelude-nix//platform:default

[project]
ignore=.git,.sl,.jj

[buck2]
file_watcher = watchman
digest_algorithms = SHA256

[buck2_re_client]
enabled = false
tls = false
instance_name = md2pdf-remote
address = grpc://127.0.0.1:8980
